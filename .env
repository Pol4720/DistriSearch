# Configuración HYPFS Docker

# Modo Docker (habilita networking entre contenedores)
DOCKER_MODE=true

# API IPFS (se sobrescribe por servicio en docker-compose)
IPFS_API_URL=http://ipfs-node-0:5001

# Configuración de red
INIT_PORT=8000
HOP_SERVER_PORT=5000

# Tamaño del hipercubo (modifica NODES = 2^HYPERCUBE_SIZE)
HYPERCUBE_SIZE=3

# Umbral para búsquedas superset
SUPERSET_THRESHOLD=10